{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/users/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/users/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/users/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/polar/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/polar/portal/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/polar/webhook/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/referral/get-url/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/user/subscription-status/?$",
			"dest": "_render"
		},
		{
			"src": "^/check-email/?$",
			"dest": "_render"
		},
		{
			"src": "^/forgot-password/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/profile/?$",
			"dest": "_render"
		},
		{
			"src": "^/reset-password/?$",
			"dest": "_render"
		},
		{
			"src": "^/signup/?$",
			"dest": "_render"
		},
		{
			"src": "^/subscription/success/?$",
			"dest": "_render"
		},
		{
			"src": "^/subscription/?$",
			"dest": "_render"
		},
		{
			"src": "^/verify-email/?$",
			"dest": "_render"
		}
	]
}